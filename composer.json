{
    "name": "saade/filament-timeline",
    "description": "This is my package filament-timeline",
    "keywords": [
        "saade",
        "laravel",
        "filament-timeline"
    ],
    "homepage": "https://github.com/saade/filament-timeline",
    "support": {
        "issues": "https://github.com/saade/filament-timeline/issues",
        "source": "https://github.com/saade/filament-timeline"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Saade",
            "email": "saade@outlook.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.0|^8.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Saade\\FilamentTimeline\\": "src/"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Saade\\FilamentTimeline\\FilamentTimelineServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
